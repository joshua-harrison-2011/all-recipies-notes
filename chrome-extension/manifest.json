{
	"name": "All Recipies Recipe Notes",
	"version": "0.01",
	"permissions": [
		"http://ubu:5000/"
	],
	"content_scripts": [
		{
			"matches": ["http://allrecipes.com/recipe/*"],
			"js": ["js/jquery-3.1.0.slim.min.js", "js/recipe-notes.js"]
		}
	],
	"web_accessible_resources": [
		"images/edit_32x32.png"
	],
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	},
	"manifest_version": 2
}
